<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.example.tempariseproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="my.example.tempariseproject.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.example.tempariseproject.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="my.example.tempariseproject.UMLActivity"
            android:label="@string/title_activity_uml" >
        </activity>
        <activity
            android:name="my.example.tempariseproject.RadarActivity"
            android:label="@string/title_activity_radar" >
        </activity>
        <activity
            android:name="my.example.tempariseproject.AllUsersActivity"
            android:label="@string/title_activity_all_users" >
        </activity>
    </application>

</manifest>
